
function insertBookingCard(innerHTML) {
const confirmedContainer = document.querySelector(".container-confirmed");
confirmedContainer.insertAdjacentHTML("afterbegin", innerHTML);
}

function removePendingCard(booking_id) {
const pendingContainer = document.querySelector(`#booking-${booking_id}`);
pendingContainer.remove();
}


insertBookingCard('<%= j render "cleaners/bookings/booking", booking: @booking %>')
removePendingCard('<%= @booking.id %>')
